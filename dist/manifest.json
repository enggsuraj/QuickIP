{
  "manifest_version": 3,
  "name": "QuickIP",
  "version": "1.0.0",
  "description": "Get user IP address, current location, time, and date with QuickIP Chrome extension.",
  "icons": {
    "16": "QuickIP.png",
    "48": "QuickIP.png",
    "128": "QuickIP.png"
  },
  "action": {
    "default_icon": {
      "16": "QuickIP.png",
      "24": "QuickIP.png",
      "32": "QuickIP.png"
    },
    "default_title": "Click Here ",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  }
}
